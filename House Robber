class Solution {
    public int rob(int[] nums) {
        if (nums.length==0) return 0;
         if (nums.length==1) return nums[0];

        int prev2=nums[0];
        int perv1=Math.max(nums[0],nums[1]);

        for(int i=2;i<nums.length;i++){
            int cur=Math.max(perv1,nums[i]+prev2);

            prev2=perv1;
            perv1=cur;
        }

       return perv1;
    }
}
